<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.emr.gds.main.medication.controller.LauncherController"
      spacing="12" alignment="CENTER" style="-fx-background-color: #f5f5f5; -fx-padding: 15;">

    <ScrollPane fitToWidth="true" VBox.vgrow="ALWAYS">
        <VBox fx:id="categoryContainer" spacing="10"/>
    </ScrollPane>

    <Separator/>

    <Button text="Quit" maxWidth="1.7976931348623157E308"
            style="-fx-background-color: #ffcccc; -fx-font-weight: bold;"
            onAction="#onQuit"/>
</VBox>
